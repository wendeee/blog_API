<%- include('partials/header.ejs'); -%>


<style>
  .banner-div, .write,.signup{
    display: none;
  };
  

</style>
  <body>
    <form class="form-group login">
      <div>
      <!-- user input-->
      <label for="email"></label>
      <input
        type="email"
        name="email"
        placeholder="Email"
        id="email"
        required
      />
      <br /><br />
      <label for="password"></label>
      <input
        type="password"
        id="password"
        name="password"
        placeholder="Password"
        required
      /><br /><br />
      <!-- submit button -->
      <button type="submit" class="btn btn-login">Login</button>
        <p class="paragraph">Do not have an account?</p>
      <a href="/signup" class="btn btn-signup">Signup</a>
    </div>
    </form>
  </body>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/axios/1.2.2/axios.min.js"></script>
  <script src="/public/Js/login.js" type="module"></script>


  <%- include('partials/footer.ejs'); -%>
</html>
